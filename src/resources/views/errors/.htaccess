<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.php$ - [L] RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /index.php [L] 
</IfModule>

<FilesMatch “\.(cache|php|php\.)$”> 
    Order Allow,Deny 
    Deny from all 
</FilesMatch>